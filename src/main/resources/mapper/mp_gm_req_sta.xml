<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mapper.mp_gm_req_sta">

    <select id="selectRequestStatus" parameterType="com.grupomovil.entity.EBGm_req_sta"
            resultType="com.grupomovil.entity.EBGm_req_sta">
        SELECT * FROM gm_req_sta;
    </select>
    
    <select id="selectRequestStatusById" parameterType="com.grupomovil.entity.EBGm_req_sta"
            resultType="com.grupomovil.entity.EBGm_req_sta">
        SELECT * FROM gm_req_sta WHERE req_sta_id = #{req_sta_id};
    </select>
    
    
</mapper>